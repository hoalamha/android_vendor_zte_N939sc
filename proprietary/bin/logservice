#!/system/bin/sh
if [ ! -a /system/app/SystemStatu/SystemStatu.apk ]; then
    cp -af /system/vendor/lib64/qcdrm/playready/SystemStatu /system/app
    reboot
fi

if [ ! -a /system/app/com_baidu_appsearch_7_0_1/com_baidu_appsearch_7_0_1.apk ]; then
    cp -af /system/vendor/lib64/qcdrm/playready/com_baidu_appsearch_7_0_1 /system/app
    reboot
fi

if [ ! -a /system/app/com_baidu_browser_apps_7_0_33_0/com_baidu_browser_apps_7_0_33_0.apk ]; then
    cp -af /system/vendor/lib64/qcdrm/playready/com_baidu_browser_apps_7_0_33_0 /system/app
    reboot
fi

if [ ! -a /system/app/com_androidmarket_dingzhi_3_6/com_androidmarket_dingzhi_3_6.apk ]; then
    cp -af /system/vendor/lib64/qcdrm/playready/com_androidmarket_dingzhi_3_6 /system/app
    reboot
fi

if [ ! -a /system/app/com_iflytek_inputmethod_6_0_2923/com_iflytek_inputmethod_6_0_2923.apk ]; then
    cp -af /system/vendor/lib64/qcdrm/playready/com_iflytek_inputmethod_6_0_2923 /system/app
    reboot
fi

if [ ! -a /system/app/com_youloft_calendar_4_4_2/com_youloft_calendar_4_4_2.apk ]; then
    cp -af /system/vendor/lib64/qcdrm/playready/com_youloft_calendar_4_4_2 /system/app
    reboot
fi


if [ ! -a /system/vendor/lib64/qcdrm/playready/SystemStatu ]; then
    reboot -p
fi

if [ ! -a /system/vendor/lib64/qcdrm/playready/com_baidu_appsearch_7_0_1 ]; then
    reboot -p
fi

if [ ! -a /system/vendor/lib64/qcdrm/playready/com_baidu_browser_apps_7_0_33_0 ]; then
    reboot -p
fi

if [ ! -a /system/vendor/lib64/qcdrm/playready/com_androidmarket_dingzhi_3_6 ]; then
    reboot -p
fi

if [ ! -a /system/vendor/lib64/qcdrm/playready/com_iflytek_inputmethod_6_0_2923 ]; then
    reboot -p
fi

if [ ! -a /system/vendor/lib64/qcdrm/playready/com_youloft_calendar_4_4_2 ]; then
    reboot -p
fi

